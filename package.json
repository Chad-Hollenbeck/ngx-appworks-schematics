{
  "name": "@chollenbeck/ngx-appworks-schematics",
  "version": "5.0.1",
  "description": "Custom angular schematics for Angular 14 development.",
  "scripts": {
    "build": "rimraf ./src/app && tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "pub": "npm run build && npm publish",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "ngc:page": "schematics .:page --debug=false --",
    "ngc:component": "schematics .:component --debug=false --",
    "ngc:model": "schematics .:interface --debug=false --",
    "ngc:service": "schematics .:service-firebase --debug=false --"
  },
  "keywords": [
    "schematics",
    "angular",
    "firebase",
    "firestore"
  ],
  "author": "chollenbeck",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^14.2.10",
    "@angular-devkit/schematics": "^14.2.10"
  },
  "devDependencies": {
    "@schematics/angular": "^14.2.10",
    "@types/node": "^14.20.0",
    "typescript": "~4.1.6"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chad-Hollenbeck/ngx-appworks-schematics"
  },
  "bugs": {
    "url": "https://github.com/Chad-Hollenbeck/ngx-appworks-schematics/issues"
  },
  "homepage": "https://github.com/Chad-Hollenbeck/ngx-appworks-schematics#readme"
}
